<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:ns1="www.tibco.com/plugin/java/JavaMethodActivityOutput" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:ns="www.tibco.com/plugin/java/JavaMethodActivityInput">
    <pd:name>Processes/TestSHA512.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startX>150</pd:startX>
    <pd:startY>64</pd:startY>
    <pd:returnBindings/>
    <pd:endName>End</pd:endName>
    <pd:endX>603</pd:endX>
    <pd:endY>63</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1527758256791</pd:targetNamespace>
    <pd:activity name="generateSHA512ForFile">
        <pd:type>com.tibco.plugin.java.JavaMethodActivity</pd:type>
        <pd:resourceType>ae.activities.JavaMethodActivity</pd:resourceType>
        <pd:x>300</pd:x>
        <pd:y>64</pd:y>
        <config>
            <ConstructDeclaredClass>false</ConstructDeclaredClass>
            <CacheConstructedClass>false</CacheConstructedClass>
            <InvokeCleanupMethod>false</InvokeCleanupMethod>
            <JavaGlobalInstance>/SharedResources/SHA512DigestHelper.serviceagent</JavaGlobalInstance>
            <MethodInfo>
                <classPath>/SharedResources/AliasLibrary.aliaslib</classPath>
                <className>com.tibco.psg.bw.crypto.SHA512DigestHelper</className>
                <methodName>generateSHA512ForFile</methodName>
                <methodReturn>java.lang.String</methodReturn>
                <methodParameter>java.lang.String</methodParameter>
            </MethodInfo>
        </config>
        <pd:inputBindings>
            <ns:JavaMethodActivityInput>
                <MethodParameters>
                    <Parameter1>
                        <xsl:value-of select="'c:/tmp/document.pdf'"/>
                    </Parameter1>
                </MethodParameters>
            </ns:JavaMethodActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="verifySHA512ForFile">
        <pd:type>com.tibco.plugin.java.JavaMethodActivity</pd:type>
        <pd:resourceType>ae.activities.JavaMethodActivity</pd:resourceType>
        <pd:x>442</pd:x>
        <pd:y>64</pd:y>
        <config>
            <ConstructDeclaredClass>false</ConstructDeclaredClass>
            <CacheConstructedClass>false</CacheConstructedClass>
            <InvokeCleanupMethod>false</InvokeCleanupMethod>
            <JavaGlobalInstance>/SharedResources/SHA512DigestHelper.serviceagent</JavaGlobalInstance>
            <MethodInfo>
                <classPath>/SharedResources/AliasLibrary.aliaslib</classPath>
                <className>com.tibco.psg.bw.crypto.SHA512DigestHelper</className>
                <methodName>verifySHA512ForFile</methodName>
                <methodReturn>boolean</methodReturn>
                <methodParameter>java.lang.String</methodParameter>
                <methodParameter>java.lang.String</methodParameter>
            </MethodInfo>
        </config>
        <pd:inputBindings>
            <ns:JavaMethodActivityInput>
                <MethodParameters>
                    <Parameter1>
                        <xsl:value-of select="$generateSHA512ForFile/ns1:JavaMethodActivityOutput/MethodReturnValue"/>
                    </Parameter1>
                    <Parameter2>
                        <xsl:value-of select="'c:/tmp/document.pdf'"/>
                    </Parameter2>
                </MethodParameters>
            </ns:JavaMethodActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>generateSHA512ForFile</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>generateSHA512ForFile</pd:from>
        <pd:to>verifySHA512ForFile</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>verifySHA512ForFile</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>